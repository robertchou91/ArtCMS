@model IEnumerable<ArtCMS.Models.ViewModels.Pages.PageVM>

@{
    string tempPageTitle = (string)TempData["PageTitle"];
}

@foreach (var item in Model)
{
    if (tempPageTitle == item.Title)
    {
        <li class="active nav-item"><a href="/@item.Slug" class="nav-link">@item.Title</a></li>
    }
    else
    {
        <li class="nav-item"><a href="/@item.Slug" class="nav-link">@item.Title</a></li>
    }

}

